{
  "name": "portfolio-sveltekit",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "prepreview:test": "vite build",
    "preview:test": "vite preview --port 3002 --strictPort",
    "test:e2e": "playwright test --config=playwright.config.ts",
    "test:e2e:ui": "playwright test --config=playwright.config.ts --ui",
    "test:smoke": "playwright test --config=playwright.config.ts --grep 'PHASE 0'",
    "test:visual:update": "playwright test --config=playwright.config.ts e2e/visual --update-snapshots",
    "show-report": "playwright show-report",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.16",
    "@vercel/analytics": "^1.1.1",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-vercel": "^5.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0"
  },
  "keywords": [],
  "author": "Nino Chavez",
  "license": "MIT"
}